{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/views/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(?:js|css|jpg|jpeg|png|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/frontend/public/$1"
    },
    {
      "src": "/(aboutus|admin|agriproduct|blog|buyer-dashboard|cancel|cart|chatbot|contactus|farmer-dashboard|financial|homepage|login|postpage|season|success|index)\\.html",
      "dest": "/frontend/views/$1.html"
    },
    {
      "src": "/",
      "dest": "/frontend/views/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/views/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}